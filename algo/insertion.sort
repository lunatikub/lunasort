InsertionSort
{
    Declaration {
        i int,
        j int,
        x int,
    }

    Implementation {
        for i <- 1 to Tsize - 1 by 1 {
            x <- T[i]
            j <- i
            while j > 0 and T[j - 1] > x {
                T[j] <- T[j - 1]
                j <- j - 1
            }
            T[j] <- x
        }
    }
}
